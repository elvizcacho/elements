{
  "name": "@allthings/elements",
  "version": "5.0.0-beta.69",
  "main": "dist/index.js",
  "module": "dist/es/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/allthings/elements",
  "author": "Moritz Schw√∂rer <moritz.schwoerer@allthings.me>",
  "license": "MIT",
  "dependencies": {
    "@allthings/colors": "4.0.1",
    "core-js": "3.6.5",
    "downshift": "3.4.8",
    "glamor": "2.20.40",
    "json2mq": "0.2.0",
    "kewler": "1.0.8",
    "match-sorter": "4.1.0",
    "mitt": "1.2.0",
    "neue": "1.0.0",
    "react-calendar": "2.19.2",
    "react-media": "1.10.0",
    "react-motion": "0.5.2"
  },
  "resolutions": {
    "marked": ">=0.6.2",
    "serialize-javascript": ">=2.1.1",
    "kind-of": ">=6.0.3",
    "minimist": ">=0.2.1 <1.0.0 || >=1.2.3",
    "yargs-parser": ">=13.1.2 <14.0.0 || >=15.0.1 <16.0.0 || >=18.1.2",
    "markdown-to-jsx": ">=6.11.4"
  },
  "peerDependencies": {
    "react": "^16.8",
    "react-dom": "^16.8"
  },
  "scripts": {
    "preversion": "yarn lint && yarn test",
    "prebuild": "rimraf dist",
    "build:es": "tsc -p tsconfig.json --target ESNext --module ESNext --outDir dist/es",
    "build:cjs": "tsc -p tsconfig.json --target ES5 --module CommonJS --outDir dist/",
    "build": "NODE_ENV=production yarn build:es && yarn build:cjs && node -r esm scripts/copy-files.js",
    "build:docs": "node -r esm ./scripts/build-docs.js",
    "release": "yarn build && yarn publish dist --new-version $npm_package_version --tag latest --access public && git push --tags origin HEAD",
    "test:example": "jest ./scripts/example.test.js",
    "watch:test": "yarn test --watch",
    "test": "yarn jest src/ scripts/",
    "jest": "NODE_ENV=test jest",
    "lint": "eslint --cache --ext .ts,.tsx src",
    "storybook": "start-storybook -p 6006",
    "security-check": "yarn audit"
  },
  "devDependencies": {
    "@storybook/addon-actions": "5.3.19",
    "@storybook/addon-links": "5.3.19",
    "@storybook/react": "5.3.19",
    "@testing-library/react": "9.5.0",
    "@types/enzyme": "3.10.5",
    "@types/jest": "25.2.3",
    "@types/json2mq": "0.2.0",
    "@types/match-sorter": "4.0.0",
    "@types/react": "16.9.37",
    "@types/react-dom": "16.9.8",
    "@types/react-motion": "0.0.29",
    "@types/react-test-renderer": "16.9.2",
    "@types/storybook__react": "4.0.2",
    "@typescript-eslint/eslint-plugin": "2.29.0",
    "@typescript-eslint/parser": "2.29.0",
    "awesome-typescript-loader": "5.2.1",
    "babel-loader": "8.1.0",
    "cross-env": "6.0.3",
    "cross-fetch": "3.0.5",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.4.4",
    "eslint": "7.4.0",
    "eslint-config-postcss": "3.0.7",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-es5": "1.5.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jest": "23.13.2",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "2.5.1",
    "eslint-plugin-standard": "4.0.1",
    "esm": "3.2.25",
    "fs-extra": "8.1.0",
    "glob": "7.1.6",
    "html-entities": "1.3.1",
    "husky": "3.1.0",
    "jest": "25.5.4",
    "jest-fetch-mock": "2.1.2",
    "jest-glamor-react": "4.3.1",
    "lint-staged": "9.5.0",
    "markdown-extract": "1.0.2",
    "path": "0.12.7",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "react": "16.13.1",
    "react-docgen": "4.1.1",
    "react-docgen-typescript": "1.17.1",
    "react-dom": "16.13.1",
    "react-test-renderer": "16.13.1",
    "rimraf": "3.0.2",
    "snapshot-diff": "0.7.0",
    "ts-jest": "25.5.1",
    "tslint": "5.20.1",
    "typescript": "3.9.5"
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "useTabs": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged && git add doc"
    }
  },
  "lint-staged": {
    "*": "yarn build:docs"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "snapshotResolver": "<rootDir>/snapshotResolver.js",
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "bail": true,
    "collectCoverage": true,
    "setupFilesAfterEnv": [
      "<rootDir>/bootstrap-jest.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer",
      "jest-glamor-react"
    ]
  }
}
